<div class="col-md-10">
  <h3>Order List</h3>
</div>
<div class="col-md-2">
  <%= link_to 'New Product', new_order_path, class: "btn btn-danger" %>
</div>
<div class="col-md-12">
  <table class="table">
    <tr>
      <% Order.column_names.each do |column| %>
      <th><%= column %></th>
      <% end %>
      <th colspan="2">Operations</th>
    </tr>
    <%= render @orders %>
  </table>
</div>