<div class="col-md-10">
  <h3><%= t(:product_list) %></h3>
</div>
<div class="col-md-2">
  <%= link_to t(:new_product), new_product_path, class: "btn btn-danger" %>
</div>
<div class="col-md-12">
  <table class="table">
    <thead>
      <tr>
        <th><%= Product.human_attribute_name("id") %></th>
        <th><%= Product.human_attribute_name("fund_id") %></th>
        <th><%= Product.human_attribute_name("name") %></th>
        <th><%= Product.human_attribute_name("desc") %></th>
        <th><%= Product.human_attribute_name("title1") %></th>
        <th><%= Product.human_attribute_name("content1") %></th>
        <th><%= Product.human_attribute_name("title2") %></th>
        <th><%= Product.human_attribute_name("content2") %></th>
        <th><%= Product.human_attribute_name("title3") %></th>
        <th><%= Product.human_attribute_name("content3") %></th>
        <th><%= Product.human_attribute_name("progress_bar") %></th>
        <th colspan="3"><%= t(:operations) %></th>
      </tr>
    </thead>
    <tbody>
      <%= render @products %>
    </tbody>
  </table>
</div>