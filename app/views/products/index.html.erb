<div class="col-md-10">
  <h3>Product List</h3>
</div>
<div class="col-md-2">
  <%= link_to 'New Product', new_product_path, class: "btn btn-danger" %>
</div>
<div class="col-md-12">
  <table class="table">
    <tr>
      <th>id</th>
      <th>fund</th>
      <th>name</th>
      <th>desc</th>
      <th>title1</th>
      <th>content1</th>
      <th>title2</th>
      <th>content2</th>
      <th>title3</th>
      <th>content3</th>
      <th>progress_bar</th>
      <th colspan="3">Operations</th>
    </tr>
    <%= render @products %>
  </table>
</div>