<tr>
  <% Institution.column_names.each do |column| %>
    <% if column == "user_id" %>
      <td><%= institution.user.name %></td>
    <% elsif column == "business_licenses" %>
      <td><%= image_tag(institution.send(column).thumb) %></td>
    <% else %>
      <td><%= institution.send(column) %></td>
    <% end %>
  <% end %>
  <td><%= link_to 'edit', edit_institution_path(institution) %></td>
  <td><%= link_to 'destroy', institution, method: :delete, data: {confirm: 'Are you sure?'} %></td>
</tr>