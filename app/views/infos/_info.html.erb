<tr>
  <td><%= info.id %></td>
  <td><%= info.category %></td>
  <td><%= info.field_name %></td>
  <td><%= info.field_type %></td>
  <td><%= info.send(info.field_type) %></td>
  <td><%= info.state %></td>
  <td><%= link_to t('destroy'), [info.order, info], method: :delete, data: {confirm: 'Are you sure?'} %></td>
</tr>