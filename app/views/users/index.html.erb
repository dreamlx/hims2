<div class="col-md-10">
  <h3>User List</h3>
</div>
<div class="col-md-2">
  <%= link_to 'New User', new_user_path, class: "btn btn-danger" %>
</div>
<div class="col-md-12">
  <table class="table">
    <tr>
      <% User.column_names.each do |column| %>
      <th><%= column %></th>
      <% end %>
      <th colspan="2">Operations</th>
    </tr>
    <%= render @users %>
  </table>
</div>